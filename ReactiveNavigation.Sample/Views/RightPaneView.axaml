<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:ReactiveNavigation.Sample.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:RightPaneViewModel" x:CompileBindings="True"
             x:Class="ReactiveNavigation.Sample.Views.RightPaneView">
  <Panel Background="LightGray" MinWidth="150">
    <StackPanel>
      <Button Content="Status" Command="{Binding ToggleStatusCommand}" Margin="4" HorizontalAlignment="Stretch" />
    </StackPanel>
  </Panel>
</UserControl>
